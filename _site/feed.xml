        &lt;span class=&quot;s&quot;&gt;&quot;J-Type 327     Light Combat   Unlimited  1         \n&quot;&lt;/span&gt; &lt;span class=&quot;p&quot;&gt;+&lt;/span&gt;
        &lt;span class=&quot;s&quot;&gt;&quot;NX Cruiser     Medium Fighter Limited    18        \n&quot;&lt;/span&gt; &lt;span class=&quot;p&quot;&gt;+&lt;/span&gt;
        &lt;span class=&quot;s&quot;&gt;&quot;N1 Starfighter Medium Fighter Unlimited  25        \n&quot;&lt;/span&gt; &lt;span class=&quot;p&quot;&gt;+&lt;/span&gt;
